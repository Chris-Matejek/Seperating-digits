package com.mycompany.mavenproject1;

/**
 *
 * @author Paul
 */
public class SepartaingDigits {
    public static void main(String[] args){
        System.out.println(4562%10);
        int num = 4562;
        while(num != 0){
            System.out.println(num%10);
            num = num/10;
        }
        
    }
           
    
}
